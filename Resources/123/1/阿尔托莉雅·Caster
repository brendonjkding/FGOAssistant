      <h3>
       <span id="战斗形象">
       </span>
       <span class="mw-headline" id=".E6.88.98.E6.96.97.E5.BD.A2.E8.B1.A1">
        战斗形象
       </span>
      </h3>
      <table class="wikitable" style="text-align:center; width:825px; display:table; white-space:normal;">
       <tbody>
        <tr>
         <th width="30px">
          模型
         </th>
         <td>
          <a class="image" href="/w/%E6%96%87%E4%BB%B6:%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png">
           <img alt="阿尔托莉雅·Caster model 1.png" class="lazyload" data-src="/images/thumb/b/b6/%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png/252px-%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png" data-srcset="/images/thumb/b/b6/%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png/378px-%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png 1.5x, /images/thumb/b/b6/%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png/504px-%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_1.png 2x" height="244" src="data:image/gif;base64,R0lGODlhAQABAAD/ACwAAAAAAQABAAACADs=" width="252"/>
          </a>
         </td>
         <td>
          <a class="image" href="/w/%E6%96%87%E4%BB%B6:%E9%98%BF%E5%B0%94%E6%89%98%E8%8E%89%E9%9B%85%C2%B7Caster_model_2.png">
