      <p>
       <script>
        var arrayTitle = new Array("第１阶段","第２阶段","第３阶段","第４阶段","愚人节","","","","","");

var count = 0;

for (n = 0; n < 10; n++) {
  if (arrayTitle[n] != "") {
    count = count + 1;
  }
}

$(document).ready(function () {
  for (i = 0; i < 10; i++) {
    if (i < count) {
      $(".graphpicker-option-" + i).text(arrayTitle[i]);
      $(".graphpicker-option-" + i).css("display", "block");
    } else {
      $(".graphpicker-option-" + i).remove();
    }
  }
});

$("select.graphpicker-select").change(function () {
  for (i = 0; i < count; i++) {
    if ($(".graphpicker-option-" + i).prop("selected") == true) {
      changeGraph(i);
      break;
    }
  }
});

$(".graphpicker-prev").click(function () {
  for (i = 0; i < count; i++) {
    if ($(".graphpicker-graph-" + i).css("display") == "block") {
      if (i == 0) {
        changeGraph(count - 1);
        changeSelect(count - 1);
      } else {
        changeGraph(i - 1);
        changeSelect(i - 1);
      }
      break;
    }
  }
});

$(".graphpicker-next").click(function () {
  for (i = 0; i < count; i++) {
    if ($(".graphpicker-graph-" + i).css("display") == "block") {
      if (i == (count - 1)) {
        changeGraph(0);
        changeSelect(0);
      } else {
        changeGraph(i + 1);
        changeSelect(i + 1);
      }
      break;
    }
  }
});

function changeGraph(p) {
  for (q = 0; q < count; q++) {
    if (q == p) {
      $(".graphpicker-graph-" + q).css("display", "block");
      $(".svtcardname-" + q).css("display", "inline");
    } else {
      $(".graphpicker-graph-" + q).css("display", "none");
      $(".svtcardname-" + q).css("display", "none");
    }
  }
}

function changeSelect(p) {
  $("select.graphpicker-select option").prop("selected", false);
  $(".graphpicker-option-" + p).prop("selected", true);
  $(".graphpicker-option-" + p).change();
}
       </script>
       <!--seostart-->
      </p>
